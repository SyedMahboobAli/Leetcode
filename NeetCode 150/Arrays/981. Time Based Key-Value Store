class TimeMap:

    def __init__(self):
        # Hash map: key → list of [timestamp, value]
        # Values are stored in increasing timestamp order by default
        self.store = {}
        

    def set(self, key: str, value: str, timestamp: int) -> None:
        if key not in self.store:
            self.store[key] = []
        self.store[key].append((timestamp,value))
        

    def get(self, key: str, timestamp: int) -> str:
        if key not in self.store:
            return ""
        arr = self.store[key]

         # Binary search to find the value with the largest timestamp ≤ targe
        l ,r = 0 , len(arr)-1
        ans = ""   # default when no timestamp <= given timestamp exists

        while l<=r:
            mid = (l+r) // 2
            if arr[mid][0] <= timestamp:
                ans = arr[mid][1] # potential answer
                l = mid + 1 # try to find a closer (larger) timestamp
            else:
                r = mid - 1
        
        return ans
        


# Your TimeMap object will be instantiated and called as such:
# obj = TimeMap()
# obj.set(key,value,timestamp)
# param_2 = obj.get(key,timestamp)
